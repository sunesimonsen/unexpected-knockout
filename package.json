{
  "name": "unexpected-knockout",
  "version": "5.5.0",
  "author": "Sune Sloth Simonsen <sune@we-knowhow.dk>",
  "keywords": [
    "assertion",
    "test",
    "assert",
    "knockout",
    "unexpected"
  ],
  "license": "MIT",
  "bugs": "https://github.com/unexpectedjs/unexpected-knockout/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/unexpectedjs/unexpected-knockout"
  },
  "scripts": {
    "lint": "eslint .",
    "test": "make test",
    "ci": "npm test && npm run lint && npm run coverage",
    "coverage": "NODE_ENV=development nyc --reporter=lcov --reporter=text --all -- npm test && echo google-chrome coverage/lcov-report/index.html",
    "generate-site": "generate-site --require ./bootstrap-unexpected-markdown.js",
    "update-examples": "generate-site --require ./bootstrap-unexpected-markdown.js --update-examples"
  },
  "main": "./lib/unexpected-knockout.js",
  "peerDependencies": {
    "knockout": "*",
    "unexpected": "5 || 6 || 7 || 8 || 9 || 10 || ^11.0.0-4"
  },
  "devDependencies": {
    "coveralls": "^3.0.2",
    "eslint": "^5.12.0",
    "eslint-config-pretty-standard": "^2.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "knockout": "^3.4.2",
    "minimist": "^1.2.0",
    "mocha": "^6.0.2",
    "mocha-slow-reporter": "*",
    "nyc": "^13.1.0",
    "prettier": "^1.15.3",
    "serve": "*",
    "unexpected": "^11.0.0-4",
    "unexpected-documentation-site-generator": "^6.0.0",
    "unexpected-markdown": "^3.0.0"
  },
  "nyc": {
    "include": [
      "lib/**"
    ]
  }
}
